{
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "buffer_font_size": 18,
  "ui_font_family": "Terminess Nerd Font Mono",
  "buffer_font_family": "Terminess Nerd Font Mono",
  "relative_line_numbers": true,
  "autosave": "on_focus_change",
  "cursor_shape": "block",
  "bottom_dock_layout": "left_aligned",
  "theme": {
    "mode": "system",
    "light": "Gruvbox Dark Hard",
    "dark": "One Dark"
  }
}
